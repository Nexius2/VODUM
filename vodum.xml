<?xml version="1.0"?>
<Container version="2">
  <Name>VODUM</Name>
  <Repository>nexius2/vodum:latest</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <ReadMe/>
  <Category>MediaApp: Tools</Category>
  <WebUI>http://[IP]:[PORT:8097]/</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Nexius2/VODUM/main/static/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1768377799</DateInstalled>
  <DonateText>Help support my work by buying me a coffee</DonateText>
  <DonateLink>https://buymeacoffee.com/vodum</DonateLink>
  <Support>https://forums.unraid.net/topic/196501-support-nexius2-vodum/</Support>
  <Project>https://github.com/Nexius2/VODUM</Project>
  <Overview>VODUM is a self-hosted subscription management tool for Plex and Jellyfin, designed to centrally manage users, subscriptions, and media library access.</Overview>
  <Requires/>
  <Config Name="Appdata" Target="/appdata" Default="/mnt/user/appdata/vodum" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/vodum</Config>
  <Config Name="TZ" Target="TZ" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">Europe/Paris</Config>
  <Config Name="Host Key 2" Target="UID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="Host Key 3" Target="GID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="WebUI Port" Target="5000" Default="8097" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8097</Config>
  <Config Name="Logs" Target="/logs" Default="/mnt/user/appdata/vodum/logs" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/vodum/logs</Config>
  <Config Name="Backups" Target="/backups" Default="/mnt/user/appdata/vodum/backups" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/vodum/backups</Config>
  <Config Name="SQLite Database Path" Target="DATABASE_PATH" Default="/appdata/database.db" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">/appdata/database.db</Config>
  <TailscaleStateDir/>
</Container>
